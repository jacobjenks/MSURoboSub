<?xml version="1.0"?>

<robot name="robosub">
	<link name="base_link">
		<visual>
			<origin xyz="0 0 0" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="1.0" radius="0.2"/>	
			</geometry>
			<material name="white">
				<color rgba=".8 .8 .8 .4"/>
			</material>
		</visual>
	</link>

	<link name="arm">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.4 0.05"/>
			</geometry>
		</visual>
	</link>

	<joint name="base_to_arm" type="revolute">
		<origin xyz="0 0 -0.2" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="arm"/>
		<limit effort="30" velocity="1" lower="0" upper="30"/>
	</joint>


	<link name="torp1">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length=".5" radius=".025"/>
			</geometry>
			<material name="black">
				<color rgba=".1 .1 .1 1"/>
			</material>
		</visual>
	</link>

	<joint name="base_to_torp1" type="fixed">
		<origin xyz="-0.1 0.3 0.2" rpy="1.5708 0 0"/>
		<parent link="base_link"/>
		<child link="torp1"/>
	</joint>

	<link name="torp2">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length=".5" radius=".025"/>
			</geometry>
			<material name="black">
				<color rgba=".1 .1 .1 1"/>
			</material>
		</visual>
	</link>

	<joint name="base_to_torp2" type="fixed">
		<origin xyz="0.1 0.3 0.2" rpy="1.5708 0 0"/>
		<parent link="base_link"/>
		<child link="torp2"/>
	</joint>

	<link name="cameraF">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size=".05 .05 .05"/>
			</geometry>
		</visual>	
	</link>

	<joint name="base_to_camF" type="fixed">
		<origin xyz="0 .5 0" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="cameraF"/>
	</joint>

	<link name="cameraD">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size=".05 .05 .05"/>
			</geometry>
		</visual>	
	</link>

	<joint name="base_to_camD" type="fixed">
		<origin xyz="0 0 -.3" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="cameraD"/>
	</joint>

	<link name="imu0">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size=".05 .05 .05"/>
			</geometry>
		</visual>
	</link>

	<joint name="base_to_imu0" type="fixed">
		<origin xyz=".1 -.3 0" rpy="0 0 0"/>	
		<parent link="base_link"/>
		<child link="imu0"/>
	</joint>

</robot>
